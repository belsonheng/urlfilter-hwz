<?xml version="1.0" encoding="UTF-8"?>
<plugin id="urlfilter-hwz" name="A URL filter for hwz"
    version="1.0.0" provider-name="belson">
 
   <runtime>
     <library name="urlfilter-hwz.jar">
       <export name="*"/>
     </library>
   </runtime>
 
   <extension id="org.apache.nutch.urlfilter.urlfilter-hwz"
       name="URL filter for hwz"
       point="org.apache.nutch.net.URLFilter">
     <implementation id="urlfilter-hwz"
         class="org.apache.nutch.urlfilter.hwz"/>
   </extension>
</plugin>
